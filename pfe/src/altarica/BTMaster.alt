//orderBack : le robot recule
//orderStop : arret du robot
//orderLeft : le robot tourne a gauche
//orderFw : le robot avance
//orderRight : le robot tourne a droite


node BTMaster
  event
    orderFw, orderBack, orderLeft, orderRight, orderStop;
  trans
    true |- orderBack
    true |- orderStop
    true |- orderLeft
    true |- orderFw
    true |- orderRight
  init
edon
  

