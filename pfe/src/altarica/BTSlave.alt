node BTSlave
  sub
    Move : Moving;
  state
    action : [-1,3] : public;
  flow
    recept : [-1,3];
  event
   back, stop, left, right, forward;
  trans
   action = -1 |- back;
   action = 0 |- stop;
   action = 1 |- left;
   action = 2 |- forward;
   action = 3 |- right;
  assert
    action = recept;
  sync
    <back, Move.back>;
    <stop, Move.stop>;
    <left, Move.left>;
    <forward, Move.forward>;
    <right, Move.right>;
edon
